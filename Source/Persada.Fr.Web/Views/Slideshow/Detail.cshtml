@model Persada.Fr.Model.GEMA_TM_SLIDESHOW
@using System.Text;

@{
    ViewBag.Title = "Detail Slideshow";
}

<h2>@ViewBag.Title.</h2>

<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h4 class="modal-title">@Model.TITTLE</h4>
        </div>
        <div class="modal-body">
            <div>
                <dl class="dl-horizontal">
                    <dt>
                        Title
                    </dt>

                    <dd>
                        @Model.TITTLE
                    </dd>

                    <dt>
                        Content
                    </dt>

                    <dd>
                        @Model.CONTENT_DESCRIPTION
                    </dd>

                    <dt>
                        Class Item
                    </dt>

                    <dd>
                        @Model.CLASS
                    </dd>

                    <dt>
                        Url
                    </dt>

                    <dd>
                        @Model.URL
                    </dd>

                    <dt>
                        Photo
                    </dt>

                    <dd>
                        <div class="col-md-12">
                            <img src="@Model.PHOTO_PATH" class="img-medium img-overflow" />
                        </div>
                    </dd>

                </dl>
            </div>

        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
    </div>
</div>
