@model IEnumerable<Persada.Fr.Model.GEMA_TM_USER> 
@using Persada.Fr.CommonFunction

@{
    ViewBag.Title = "Index News";
    ViewBag.ActiveNews = "open active";
    ViewBag.SelectedNews = "selected";
    ViewBag.BreadcrumbMenu = "News > ";
    ViewBag.BreadcrumbSubMenu = "Index > ";
}

<link href="~/Content/Metronics/global/plugins/scripts/jquery.notific8.min.css" rel="stylesheet" />
<link href="~/Content/Metronics/global/plugins/scripts/toastr.min.css" rel="stylesheet" />

@if (ViewBag.msgSuccess != null)
{
    <div class="alert alert-success">
        <a href="#" class="close" type="button" data-dismiss="alert">
            <i class="ace-icon fa fa-times"></i>
        </a>

        <strong>
            <i class="ace-icon fa fa-check"></i>
        </strong>
        @ViewBag.msgSuccess
        <br>
    </div>
}

@if (ViewBag.msgError != null)
{
    <div class="alert alert-danger">
        <a href="#" class="close" type="button" data-dismiss="alert">
            <i class="ace-icon fa fa-times"></i>
        </a>
        <strong>
            <i class="ace-icon fa fa-times"></i>
        </strong>
        @ViewBag.msgError
        <br>
    </div>
} 

<div class="row">
    <div class="col-md-6">
        <!-- BEGIN Portlet PORTLET-->
        <div class="portlet box yellow">
            <div class="portlet-title">
                <div class="caption">
                </div>
                <div class="tools">
                    <a href="javascript:;" class="collapse" data-original-title="" title=""><i class="icon-drop"></i></a>
                    <a href="javascript:;" data-error-display="notific8" data-url="error" class="reload" data-original-title="" title=""><i class="icon-cloud-upload"></i></a>
                    <a href="javascript:;" class="" data-original-title="" title=""> <i class="icon-size-fullscreen"></i> </a>
                    <a href="javascript:;" class="remove" data-original-title="" title=""> <i class="icon-trash"></i></a>
                </div>
            </div>
            <div class="portlet-body portlet-empty"> Try to reload the content to see how the error message is displayed. </div>
        </div>
        <!-- END Portlet PORTLET-->
    </div>
    <div class="col-md-6">
        <!-- BEGIN Portlet PORTLET-->
        <div class="portlet box purple">
            <div class="portlet-title">
                <div class="caption">
                </div>
